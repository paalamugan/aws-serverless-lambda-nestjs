{
  "name": "aws-serverless-lambda-nestjs",
  "version": "0.0.1",
  "description": "AWS serverless lambda function for send customer information to google form through api using nestjs",
  "author": "paalamugan",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:watch": "yarn start --watch",
    "start:debug": "yarn start:watch --debug",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "dev": "yarn start:watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn test --config ./test/jest-e2e.json",
    "sls:offline": "sls offline",
    "sls:package": "sls package",
    "sls:deploy": "sls deploy",
    "sls:deploy:prod": "yarn sls:deploy --stage production",
    "sls:deploy:devProfile": "yarn sls:deploy --aws-profile devProfile",
    "sls:deploy:prodProfile": "yarn sls:deploy:prod --aws-profile prodProfile",
    "sls:print": "sls print"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "js-yaml": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "serverless": "^3.26.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "^12.0.3",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.19",
    "npm": "please use yarn"
  },
  "packageManager": "yarn@1.22.19",
  "keywords": [
    "nestjs",
    "serverless",
    "aws",
    "lambda",
    "google form"
  ]
}